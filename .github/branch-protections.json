{
  "branches": [
    {
      "name": "main",
      "protection": {
        "required_status_checks": {
          "strict": true,
          "contexts": ["quality", "test", "security", "build"]
        },
        "required_pull_request_reviews": {
          "required_approving_review_count": 1,
          "dismiss_stale_reviews": true
        },
        "enforce_admins": true,
        "restrictions": null
      }
    },
    {
      "name": "develop",
      "protection": {
        "required_status_checks": {
          "strict": true,
          "contexts": ["quality", "test", "security"]
        }
      }
    }
  ]
}
